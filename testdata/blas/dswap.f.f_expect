
 SUBROUTINE DSWAP ( N , DX , INCX , DY , INCY )
 INTEGER INCX , INCY , N
 DOUBLE PRECISION DX ( * ) , DY ( * )
 DOUBLE PRECISION DTEMP
 INTEGER I , IX , IY , M , MP1
 INTRINSIC MOD
 IF ( N .LE. 0 ) RETURN
 IF ( INCX .EQ. 1 .AND. INCY .EQ. 1 ) THEN
 M = MOD ( N , 3 )
 IF ( M .NE. 0 ) THEN
 DO I = 1 , M
 DTEMP = DX ( I )
 DX ( I ) = DY ( I )
 DY ( I ) = DTEMP
 END DO
 IF ( N .LT. 3 ) RETURN
 END IF
 MP1 = M + 1
 DO I = MP1 , N , 3
 DTEMP = DX ( I )
 DX ( I ) = DY ( I )
 DY ( I ) = DTEMP
 DTEMP = DX ( I + 1 )
 DX ( I + 1 ) = DY ( I + 1 )
 DY ( I + 1 ) = DTEMP
 DTEMP = DX ( I + 2 )
 DX ( I + 2 ) = DY ( I + 2 )
 DY ( I + 2 ) = DTEMP
 END DO
 ELSE
 IX = 1
 IY = 1
 IF ( INCX .LT. 0 ) IX = ( - N + 1 ) * INCX + 1
 IF ( INCY .LT. 0 ) IY = ( - N + 1 ) * INCY + 1
 DO I = 1 , N
 DTEMP = DX ( IX )
 DX ( IX ) = DY ( IY )
 DY ( IY ) = DTEMP
 IX = IX + INCX
 IY = IY + INCY
 END DO
 END IF
 RETURN
 END
