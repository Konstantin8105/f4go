 SUBROUTINE CGEMM ( TRANSA , TRANSB , M , N , K , ALPHA , A , LDA , B , LDB , BETA , C , LDC )
 COMPLEX ALPHA , BETA
 INTEGER K , LDA , LDB , LDC , M , N
 CHARACTER TRANSA , TRANSB
 COMPLEX A ( LDA , * ) , B ( LDB , * ) , C ( LDC , * )
 LOGICAL LSAME
 EXTERNAL LSAME
 EXTERNAL XERBLA
 INTRINSIC CONJG , MAX
 COMPLEX TEMP
 INTEGER I , INFO , J , L , NCOLA , NROWA , NROWB
 LOGICAL CONJA , CONJB , NOTA , NOTB
 COMPLEX ONE


 ONE = ( 1.0E+0 , 0.0E+0 )

 COMPLEX ZERO


 ZERO = ( 0.0E+0 , 0.0E+0 )

 NOTA = LSAME ( TRANSA , 'N' )
 NOTB = LSAME ( TRANSB , 'N' )
 CONJA = LSAME ( TRANSA , 'C' )
 CONJB = LSAME ( TRANSB , 'C' )
 IF ( NOTA ) THEN
 NROWA = M
 NCOLA = K
 ELSE
 NROWA = K
 NCOLA = M
 END
 IF ( NOTB ) THEN
 NROWB = K
 ELSE
 NROWB = N
 END
 INFO = 0
 IF ( ( .NOT. NOTA ) .AND. ( .NOT. CONJA ) .AND. ( .NOT. LSAME ( TRANSA , 'T' ) ) ) THEN
 INFO = 1
 ELSE IF ( ( .NOT. NOTB ) .AND. ( .NOT. CONJB ) .AND. ( .NOT. LSAME ( TRANSB , 'T' ) ) ) THEN
 INFO = 2
 ELSE IF ( M .LT. 0 ) THEN
 INFO = 3
 ELSE IF ( N .LT. 0 ) THEN
 INFO = 4
 ELSE IF ( K .LT. 0 ) THEN
 INFO = 5
 ELSE IF ( LDA .LT. MAX ( 1 , NROWA ) ) THEN
 INFO = 8
 ELSE IF ( LDB .LT. MAX ( 1 , NROWB ) ) THEN
 INFO = 10
 ELSE IF ( LDC .LT. MAX ( 1 , M ) ) THEN
 INFO = 13
 END
 IF ( INFO .NE. 0 ) THEN
 CALL XERBLA ( 'CGEMM ' , INFO )
 RETURN
 END
 IF ( ( M .EQ. 0 ) .OR. ( N .EQ. 0 ) .OR. ( ( ( ALPHA .EQ. ZERO ) .OR. ( K .EQ. 0 ) ) .AND. ( BETA .EQ. ONE ) ) ) RETURN
 IF ( ALPHA .EQ. ZERO ) THEN
 IF ( BETA .EQ. ZERO ) THEN
 DO J = 1 , N
 DO I = 1 , M
 C ( I , J ) = ZERO
 end

 end

 ELSE
 DO J = 1 , N
 DO I = 1 , M
 C ( I , J ) = BETA * C ( I , J )
 end

 end

 END
 RETURN
 END
 IF ( NOTB ) THEN
 IF ( NOTA ) THEN
 DO J = 1 , N
 IF ( BETA .EQ. ZERO ) THEN
 DO I = 1 , M
 C ( I , J ) = ZERO
 end

 ELSE IF ( BETA .NE. ONE ) THEN
 DO I = 1 , M
 C ( I , J ) = BETA * C ( I , J )
 end

 END
 DO L = 1 , K
 TEMP = ALPHA * B ( L , J )
 DO I = 1 , M
 C ( I , J ) = C ( I , J ) + TEMP * A ( I , L )
 end

 end

 end

 ELSE IF ( CONJA ) THEN
 DO J = 1 , N
 DO I = 1 , M
 TEMP = ZERO
 DO L = 1 , K
 TEMP = TEMP + CONJG ( A ( L , I ) ) * B ( L , J )
 end

 IF ( BETA .EQ. ZERO ) THEN
 C ( I , J ) = ALPHA * TEMP
 ELSE
 C ( I , J ) = ALPHA * TEMP + BETA * C ( I , J )
 END
 end

 end

 ELSE
 DO J = 1 , N
 DO I = 1 , M
 TEMP = ZERO
 DO L = 1 , K
 TEMP = TEMP + A ( L , I ) * B ( L , J )
 end

 IF ( BETA .EQ. ZERO ) THEN
 C ( I , J ) = ALPHA * TEMP
 ELSE
 C ( I , J ) = ALPHA * TEMP + BETA * C ( I , J )
 END
 end

 end

 END
 ELSE IF ( NOTA ) THEN
 IF ( CONJB ) THEN
 DO J = 1 , N
 IF ( BETA .EQ. ZERO ) THEN
 DO I = 1 , M
 C ( I , J ) = ZERO
 end

 ELSE IF ( BETA .NE. ONE ) THEN
 DO I = 1 , M
 C ( I , J ) = BETA * C ( I , J )
 end

 END
 DO L = 1 , K
 TEMP = ALPHA * CONJG ( B ( J , L ) )
 DO I = 1 , M
 C ( I , J ) = C ( I , J ) + TEMP * A ( I , L )
 end

 end

 end

 ELSE
 DO J = 1 , N
 IF ( BETA .EQ. ZERO ) THEN
 DO I = 1 , M
 C ( I , J ) = ZERO
 end

 ELSE IF ( BETA .NE. ONE ) THEN
 DO I = 1 , M
 C ( I , J ) = BETA * C ( I , J )
 end

 END
 DO L = 1 , K
 TEMP = ALPHA * B ( J , L )
 DO I = 1 , M
 C ( I , J ) = C ( I , J ) + TEMP * A ( I , L )
 end

 end

 end

 END
 ELSE IF ( CONJA ) THEN
 IF ( CONJB ) THEN
 DO J = 1 , N
 DO I = 1 , M
 TEMP = ZERO
 DO L = 1 , K
 TEMP = TEMP + CONJG ( A ( L , I ) ) * CONJG ( B ( J , L ) )
 end

 IF ( BETA .EQ. ZERO ) THEN
 C ( I , J ) = ALPHA * TEMP
 ELSE
 C ( I , J ) = ALPHA * TEMP + BETA * C ( I , J )
 END
 end

 end

 ELSE
 DO J = 1 , N
 DO I = 1 , M
 TEMP = ZERO
 DO L = 1 , K
 TEMP = TEMP + CONJG ( A ( L , I ) ) * B ( J , L )
 end

 IF ( BETA .EQ. ZERO ) THEN
 C ( I , J ) = ALPHA * TEMP
 ELSE
 C ( I , J ) = ALPHA * TEMP + BETA * C ( I , J )
 END
 end

 end

 END
 ELSE
 IF ( CONJB ) THEN
 DO J = 1 , N
 DO I = 1 , M
 TEMP = ZERO
 DO L = 1 , K
 TEMP = TEMP + A ( L , I ) * CONJG ( B ( J , L ) )
 end

 IF ( BETA .EQ. ZERO ) THEN
 C ( I , J ) = ALPHA * TEMP
 ELSE
 C ( I , J ) = ALPHA * TEMP + BETA * C ( I , J )
 END
 end

 end

 ELSE
 DO J = 1 , N
 DO I = 1 , M
 TEMP = ZERO
 DO L = 1 , K
 TEMP = TEMP + A ( L , I ) * B ( J , L )
 end

 IF ( BETA .EQ. ZERO ) THEN
 C ( I , J ) = ALPHA * TEMP
 ELSE
 C ( I , J ) = ALPHA * TEMP + BETA * C ( I , J )
 END
 end

 end

 END
 END
 RETURN
 END
