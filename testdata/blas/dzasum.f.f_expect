 DOUBLE PRECISION FUNCTION DZASUM ( N , ZX , INCX )
 INTEGER INCX , N
 COMPLEX * 16 ZX ( * )
 DOUBLE PRECISION STEMP
 INTEGER I , NINCX
 DOUBLE PRECISION DCABS1
 EXTERNAL DCABS1
 DZASUM = 0.0d0
 STEMP = 0.0d0
 IF ( N .LE. 0 .OR. INCX .LE. 0 ) RETURN
 IF ( INCX .EQ. 1 ) THEN
 DO I = 1 , N
 STEMP = STEMP + DCABS1 ( ZX ( I ) )
 END
 ELSE
 NINCX = N * INCX
 DO I = 1 , NINCX , INCX
 STEMP = STEMP + DCABS1 ( ZX ( I ) )
 END
 END
 DZASUM = STEMP
 RETURN
 END
