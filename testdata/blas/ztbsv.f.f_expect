 SUBROUTINE ZTBSV ( UPLO , TRANS , DIAG , N , K , A , LDA , X , INCX )
 INTEGER INCX , K , LDA , N
 CHARACTER DIAG , TRANS , UPLO
 COMPLEX * 16 A ( LDA , * ) , X ( * )
 COMPLEX * 16 ZERO
 PARAMETER ( ZERO = ( 0.0D+0 , 0.0D+0 ) )
 COMPLEX * 16 TEMP
 INTEGER I , INFO , IX , J , JX , KPLUS1 , KX , L
 LOGICAL NOCONJ , NOUNIT
 LOGICAL LSAME
 EXTERNAL LSAME
 EXTERNAL XERBLA
 INTRINSIC DCONJG , MAX , MIN
 INFO = 0
 IF ( .NOT. LSAME ( UPLO , 'U' ) .AND. .NOT. LSAME ( UPLO , 'L' ) ) THEN
 INFO = 1
 ELSE IF ( .NOT. LSAME ( TRANS , 'N' ) .AND. .NOT. LSAME ( TRANS , 'T' ) .AND. .NOT. LSAME ( TRANS , 'C' ) ) THEN
 INFO = 2
 ELSE IF ( .NOT. LSAME ( DIAG , 'U' ) .AND. .NOT. LSAME ( DIAG , 'N' ) ) THEN
 INFO = 3
 ELSE IF ( N .LT. 0 ) THEN
 INFO = 4
 ELSE IF ( K .LT. 0 ) THEN
 INFO = 5
 ELSE IF ( LDA .LT. ( K + 1 ) ) THEN
 INFO = 7
 ELSE IF ( INCX .EQ. 0 ) THEN
 INFO = 9
 END
 IF ( INFO .NE. 0 ) THEN
 CALL XERBLA ( 'ZTBSV ' , INFO )
 RETURN
 END
 IF ( N .EQ. 0 ) RETURN
 NOCONJ = LSAME ( TRANS , 'T' )
 NOUNIT = LSAME ( DIAG , 'N' )
 IF ( INCX .LE. 0 ) THEN
 KX = 1 - ( N - 1 ) * INCX
 ELSE IF ( INCX .NE. 1 ) THEN
 KX = 1
 END
 IF ( LSAME ( TRANS , 'N' ) ) THEN
 IF ( LSAME ( UPLO , 'U' ) ) THEN
 KPLUS1 = K + 1
 IF ( INCX .EQ. 1 ) THEN
 DO J = N , 1 , - 1
 IF ( X ( J ) .NE. ZERO ) THEN
 L = KPLUS1 - J
 IF ( NOUNIT ) X ( J ) = X ( J ) / A ( KPLUS1 , J )
 TEMP = X ( J )
 DO I = J - 1 , MAX ( 1 , J - K ) , - 1
 X ( I ) = X ( I ) - TEMP * A ( L + I , J )
 end

 END
 end

 ELSE
 KX = KX + ( N - 1 ) * INCX
 JX = KX
 DO J = N , 1 , - 1
 KX = KX - INCX
 IF ( X ( JX ) .NE. ZERO ) THEN
 IX = KX
 L = KPLUS1 - J
 IF ( NOUNIT ) X ( JX ) = X ( JX ) / A ( KPLUS1 , J )
 TEMP = X ( JX )
 DO I = J - 1 , MAX ( 1 , J - K ) , - 1
 X ( IX ) = X ( IX ) - TEMP * A ( L + I , J )
 IX = IX - INCX
 end

 END
 JX = JX - INCX
 end

 END
 ELSE
 IF ( INCX .EQ. 1 ) THEN
 DO J = 1 , N
 IF ( X ( J ) .NE. ZERO ) THEN
 L = 1 - J
 IF ( NOUNIT ) X ( J ) = X ( J ) / A ( 1 , J )
 TEMP = X ( J )
 DO I = J + 1 , MIN ( N , J + K )
 X ( I ) = X ( I ) - TEMP * A ( L + I , J )
 end

 END
 end

 ELSE
 JX = KX
 DO J = 1 , N
 KX = KX + INCX
 IF ( X ( JX ) .NE. ZERO ) THEN
 IX = KX
 L = 1 - J
 IF ( NOUNIT ) X ( JX ) = X ( JX ) / A ( 1 , J )
 TEMP = X ( JX )
 DO I = J + 1 , MIN ( N , J + K )
 X ( IX ) = X ( IX ) - TEMP * A ( L + I , J )
 IX = IX + INCX
 end

 END
 JX = JX + INCX
 end

 END
 END
 ELSE
 IF ( LSAME ( UPLO , 'U' ) ) THEN
 KPLUS1 = K + 1
 IF ( INCX .EQ. 1 ) THEN
 DO J = 1 , N
 TEMP = X ( J )
 L = KPLUS1 - J
 IF ( NOCONJ ) THEN
 DO I = MAX ( 1 , J - K ) , J - 1
 TEMP = TEMP - A ( L + I , J ) * X ( I )
 end

 IF ( NOUNIT ) TEMP = TEMP / A ( KPLUS1 , J )
 ELSE
 DO I = MAX ( 1 , J - K ) , J - 1
 TEMP = TEMP - DCONJG ( A ( L + I , J ) ) * X ( I )
 end

 IF ( NOUNIT ) TEMP = TEMP / DCONJG ( A ( KPLUS1 , J ) )
 END
 X ( J ) = TEMP
 end

 ELSE
 JX = KX
 DO J = 1 , N
 TEMP = X ( JX )
 IX = KX
 L = KPLUS1 - J
 IF ( NOCONJ ) THEN
 DO I = MAX ( 1 , J - K ) , J - 1
 TEMP = TEMP - A ( L + I , J ) * X ( IX )
 IX = IX + INCX
 end

 IF ( NOUNIT ) TEMP = TEMP / A ( KPLUS1 , J )
 ELSE
 DO I = MAX ( 1 , J - K ) , J - 1
 TEMP = TEMP - DCONJG ( A ( L + I , J ) ) * X ( IX )
 IX = IX + INCX
 end

 IF ( NOUNIT ) TEMP = TEMP / DCONJG ( A ( KPLUS1 , J ) )
 END
 X ( JX ) = TEMP
 JX = JX + INCX
 IF ( J .GT. K ) KX = KX + INCX
 end

 END
 ELSE
 IF ( INCX .EQ. 1 ) THEN
 DO J = N , 1 , - 1
 TEMP = X ( J )
 L = 1 - J
 IF ( NOCONJ ) THEN
 DO I = MIN ( N , J + K ) , J + 1 , - 1
 TEMP = TEMP - A ( L + I , J ) * X ( I )
 end

 IF ( NOUNIT ) TEMP = TEMP / A ( 1 , J )
 ELSE
 DO I = MIN ( N , J + K ) , J + 1 , - 1
 TEMP = TEMP - DCONJG ( A ( L + I , J ) ) * X ( I )
 end

 IF ( NOUNIT ) TEMP = TEMP / DCONJG ( A ( 1 , J ) )
 END
 X ( J ) = TEMP
 end

 ELSE
 KX = KX + ( N - 1 ) * INCX
 JX = KX
 DO J = N , 1 , - 1
 TEMP = X ( JX )
 IX = KX
 L = 1 - J
 IF ( NOCONJ ) THEN
 DO I = MIN ( N , J + K ) , J + 1 , - 1
 TEMP = TEMP - A ( L + I , J ) * X ( IX )
 IX = IX - INCX
 end

 IF ( NOUNIT ) TEMP = TEMP / A ( 1 , J )
 ELSE
 DO I = MIN ( N , J + K ) , J + 1 , - 1
 TEMP = TEMP - DCONJG ( A ( L + I , J ) ) * X ( IX )
 IX = IX - INCX
 end

 IF ( NOUNIT ) TEMP = TEMP / DCONJG ( A ( 1 , J ) )
 END
 X ( JX ) = TEMP
 JX = JX - INCX
 IF ( ( N - J ) .GE. K ) KX = KX - INCX
 end

 END
 END
 END
 RETURN
 END
