NEW_LINE            	

COMMENT             	*> \verbatim
NEW_LINE            	

COMMENT             	*>          SRNAME is CHARACTER*(*)
NEW_LINE            	

COMMENT             	*>          The name of the routine which called XERBLA.
NEW_LINE            	

COMMENT             	*> \endverbatim
NEW_LINE            	

COMMENT             	*>
NEW_LINE            	

COMMENT             	*> \param[in] INFO
NEW_LINE            	

COMMENT             	*> \verbatim
NEW_LINE            	

COMMENT             	*>          INFO is INTEGER
NEW_LINE            	

COMMENT             	*>          The position of the invalid parameter in the parameter list
NEW_LINE            	

COMMENT             	*>          of the calling routine.
NEW_LINE            	

COMMENT             	*> \endverbatim
