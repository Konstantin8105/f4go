COMMENT             	*
if                  	IF
(                   	(
IDENT               	INTA
>=                  	.GE.
INT                 	225
&&                  	.AND.
IDENT               	INTA
<=                  	.LE.
INT                 	250
)                   	)
IDENT               	INTA
=                   	=
IDENT               	INTA
-                   	-
INT                 	32
if                  	IF
(                   	(
IDENT               	INTB
>=                  	.GE.
INT                 	225
&&                  	.AND.
IDENT               	INTB
<=                  	.LE.
INT                 	250
)                   	)
IDENT               	INTB
=                   	=
IDENT               	INTB
