IDENT               	INTA
>=                  	.GE.
INT                 	162
&&                  	.AND.
IDENT               	INTA
<=                  	.LE.
INT                 	169
)                   	)
IDENT               	INTA
=                   	=
IDENT               	INTA
+                   	+
INT                 	64
if                  	IF
(                   	(
IDENT               	INTB
>=                  	.GE.
INT                 	129
&&                  	.AND.
IDENT               	INTB
<=                  	.LE.
INT                 	137
||                  	.OR.
+                   	+
IDENT               	INTB
>=                  	.GE.
INT                 	145
&&                  	.AND.
IDENT               	INTB
<=                  	.LE.
INT                 	153
||                  	.OR.
+                   	+
IDENT               	INTB
>=                  	.GE.
INT                 	162
&&                  	.AND.
IDENT               	INTB
<=                  	.LE.
INT                 	169
)                   	)
IDENT               	INTB
=                   	=
IDENT               	INTB
+                   	+
INT                 	64
