
 REAL FUNCTION SCASUM ( N , CX , INCX )
 INTEGER INCX , N
 COMPLEX CX ( * )
 REAL STEMP
 INTEGER I , NINCX
 INTRINSIC ABS , AIMAG , REAL
 SCASUM = 0.0e0
 STEMP = 0.0e0
 IF ( N .LE. 0 .OR. INCX .LE. 0 ) RETURN
 IF ( INCX .EQ. 1 ) THEN
 DO I = 1 , N
 STEMP = STEMP + ABS ( REAL ( CX ( I ) ) ) + ABS ( AIMAG ( CX ( I ) ) )
 END DO
 ELSE
 NINCX = N * INCX
 DO I = 1 , NINCX , INCX
 STEMP = STEMP + ABS ( REAL ( CX ( I ) ) ) + ABS ( AIMAG ( CX ( I ) ) )
 END DO
 END IF
 SCASUM = STEMP
 RETURN
 END
