 SUBROUTINE CCOPY ( N , CX , INCX , CY , INCY )
 INTEGER INCX , INCY , N
 COMPLEX CX ( * ) , CY ( * )
 INTEGER I , IX , IY
 IF ( N .LE. 0 ) RETURN
 IF ( INCX .EQ. 1 .AND. INCY .EQ. 1 ) THEN
 DO I = 1 , N
 CY ( I ) = CX ( I )
 END DO
 ELSE
 IX = 1
 IY = 1
 IF ( INCX .LT. 0 ) IX = ( - N + 1 ) * INCX + 1
 IF ( INCY .LT. 0 ) IY = ( - N + 1 ) * INCY + 1
 DO I = 1 , N
 CY ( IY ) = CX ( IX )
 IX = IX + INCX
 IY = IY + INCY
 END DO
 END
 RETURN
 END
