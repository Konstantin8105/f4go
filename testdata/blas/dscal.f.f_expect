 SUBROUTINE DSCAL ( N , DA , DX , INCX )
 DOUBLE PRECISION DA
 INTEGER INCX , N
 DOUBLE PRECISION DX ( * )
 INTEGER I , M , MP1 , NINCX
 INTRINSIC MOD
 IF ( N .LE. 0 .OR. INCX .LE. 0 ) RETURN
 IF ( INCX .EQ. 1 ) THEN
 M = MOD ( N , 5 )
 IF ( M .NE. 0 ) THEN
 DO I = 1 , M
 DX ( I ) = DA * DX ( I )
 END
 IF ( N .LT. 5 ) RETURN
 END
 MP1 = M + 1
 DO I = MP1 , N , 5
 DX ( I ) = DA * DX ( I )
 DX ( I + 1 ) = DA * DX ( I + 1 )
 DX ( I + 2 ) = DA * DX ( I + 2 )
 DX ( I + 3 ) = DA * DX ( I + 3 )
 DX ( I + 4 ) = DA * DX ( I + 4 )
 END
 ELSE
 NINCX = N * INCX
 DO I = 1 , NINCX , INCX
 DX ( I ) = DA * DX ( I )
 END
 END
 RETURN
 END
