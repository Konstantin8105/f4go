 SUBROUTINE CGERU ( M , N , ALPHA , X , INCX , Y , INCY , A , LDA )
 COMPLEX ALPHA
 INTEGER INCX , INCY , LDA , M , N
 COMPLEX A ( LDA , * ) , X ( * ) , Y ( * )
 COMPLEX ZERO


 ZERO = ( 0.0E+0 , 0.0E+0 )

 COMPLEX TEMP
 INTEGER I , INFO , IX , J , JY , KX
 EXTERNAL XERBLA
 INTRINSIC MAX
 INFO = 0
 IF ( M .LT. 0 ) THEN
 INFO = 1
 ELSE IF ( N .LT. 0 ) THEN
 INFO = 2
 ELSE IF ( INCX .EQ. 0 ) THEN
 INFO = 5
 ELSE IF ( INCY .EQ. 0 ) THEN
 INFO = 7
 ELSE IF ( LDA .LT. MAX ( 1 , M ) ) THEN
 INFO = 9
 END
 IF ( INFO .NE. 0 ) THEN
 CALL XERBLA ( 'CGERU ' , INFO )
 RETURN
 END
 IF ( ( M .EQ. 0 ) .OR. ( N .EQ. 0 ) .OR. ( ALPHA .EQ. ZERO ) ) RETURN
 IF ( INCY .GT. 0 ) THEN
 JY = 1
 ELSE
 JY = 1 - ( N - 1 ) * INCY
 END
 IF ( INCX .EQ. 1 ) THEN
 DO J = 1 , N
 IF ( Y ( JY ) .NE. ZERO ) THEN
 TEMP = ALPHA * Y ( JY )
 DO I = 1 , M
 A ( I , J ) = A ( I , J ) + X ( I ) * TEMP
 end

 END
 JY = JY + INCY
 end

 ELSE
 IF ( INCX .GT. 0 ) THEN
 KX = 1
 ELSE
 KX = 1 - ( M - 1 ) * INCX
 END
 DO J = 1 , N
 IF ( Y ( JY ) .NE. ZERO ) THEN
 TEMP = ALPHA * Y ( JY )
 IX = KX
 DO I = 1 , M
 A ( I , J ) = A ( I , J ) + X ( IX ) * TEMP
 IX = IX + INCX
 end

 END
 JY = JY + INCY
 end

 END
 RETURN
 END
