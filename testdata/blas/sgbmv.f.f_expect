 SUBROUTINE SGBMV ( TRANS , M , N , KL , KU , ALPHA , A , LDA , X , INCX , BETA , Y , INCY )
 REAL ALPHA , BETA
 INTEGER INCX , INCY , KL , KU , LDA , M , N
 CHARACTER TRANS
 REAL A ( LDA , * ) , X ( * ) , Y ( * )
 REAL ONE , ZERO
 PARAMETER ( ONE = 1.0E+0 , ZERO = 0.0E+0 )
 REAL TEMP
 INTEGER I , INFO , IX , IY , J , JX , JY , K , KUP1 , KX , KY , LENX , LENY
 LOGICAL LSAME
 EXTERNAL LSAME
 EXTERNAL XERBLA
 INTRINSIC MAX , MIN
 INFO = 0
 IF ( .NOT. LSAME ( TRANS , 'N' ) .AND. .NOT. LSAME ( TRANS , 'T' ) .AND. .NOT. LSAME ( TRANS , 'C' ) ) THEN
 INFO = 1
 ELSE IF ( M .LT. 0 ) THEN
 INFO = 2
 ELSE IF ( N .LT. 0 ) THEN
 INFO = 3
 ELSE IF ( KL .LT. 0 ) THEN
 INFO = 4
 ELSE IF ( KU .LT. 0 ) THEN
 INFO = 5
 ELSE IF ( LDA .LT. ( KL + KU + 1 ) ) THEN
 INFO = 8
 ELSE IF ( INCX .EQ. 0 ) THEN
 INFO = 10
 ELSE IF ( INCY .EQ. 0 ) THEN
 INFO = 13
 END
 IF ( INFO .NE. 0 ) THEN
 CALL XERBLA ( 'SGBMV ' , INFO )
 RETURN
 END
 IF ( ( M .EQ. 0 ) .OR. ( N .EQ. 0 ) .OR. ( ( ALPHA .EQ. ZERO ) .AND. ( BETA .EQ. ONE ) ) ) RETURN
 IF ( LSAME ( TRANS , 'N' ) ) THEN
 LENX = N
 LENY = M
 ELSE
 LENX = M
 LENY = N
 END
 IF ( INCX .GT. 0 ) THEN
 KX = 1
 ELSE
 KX = 1 - ( LENX - 1 ) * INCX
 END
 IF ( INCY .GT. 0 ) THEN
 KY = 1
 ELSE
 KY = 1 - ( LENY - 1 ) * INCY
 END
 IF ( BETA .NE. ONE ) THEN
 IF ( INCY .EQ. 1 ) THEN
 IF ( BETA .EQ. ZERO ) THEN
 DO I = 1 , LENY
 Y ( I ) = ZERO
 end

 ELSE
 DO I = 1 , LENY
 Y ( I ) = BETA * Y ( I )
 end

 END
 ELSE
 IY = KY
 IF ( BETA .EQ. ZERO ) THEN
 DO I = 1 , LENY
 Y ( IY ) = ZERO
 IY = IY + INCY
 end

 ELSE
 DO I = 1 , LENY
 Y ( IY ) = BETA * Y ( IY )
 IY = IY + INCY
 end

 END
 END
 END
 IF ( ALPHA .EQ. ZERO ) RETURN
 KUP1 = KU + 1
 IF ( LSAME ( TRANS , 'N' ) ) THEN
 JX = KX
 IF ( INCY .EQ. 1 ) THEN
 DO J = 1 , N
 TEMP = ALPHA * X ( JX )
 K = KUP1 - J
 DO I = MAX ( 1 , J - KU ) , MIN ( M , J + KL )
 Y ( I ) = Y ( I ) + TEMP * A ( K + I , J )
 end

 JX = JX + INCX
 end

 ELSE
 DO J = 1 , N
 TEMP = ALPHA * X ( JX )
 IY = KY
 K = KUP1 - J
 DO I = MAX ( 1 , J - KU ) , MIN ( M , J + KL )
 Y ( IY ) = Y ( IY ) + TEMP * A ( K + I , J )
 IY = IY + INCY
 end

 JX = JX + INCX
 IF ( J .GT. KU ) KY = KY + INCY
 end

 END
 ELSE
 JY = KY
 IF ( INCX .EQ. 1 ) THEN
 DO J = 1 , N
 TEMP = ZERO
 K = KUP1 - J
 DO I = MAX ( 1 , J - KU ) , MIN ( M , J + KL )
 TEMP = TEMP + A ( K + I , J ) * X ( I )
 end

 Y ( JY ) = Y ( JY ) + ALPHA * TEMP
 JY = JY + INCY
 end

 ELSE
 DO J = 1 , N
 TEMP = ZERO
 IX = KX
 K = KUP1 - J
 DO I = MAX ( 1 , J - KU ) , MIN ( M , J + KL )
 TEMP = TEMP + A ( K + I , J ) * X ( IX )
 IX = IX + INCX
 end

 Y ( JY ) = Y ( JY ) + ALPHA * TEMP
 JY = JY + INCY
 IF ( J .GT. KU ) KX = KX + INCX
 end

 END
 END
 RETURN
 END
