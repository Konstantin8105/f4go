 SUBROUTINE ZSCAL ( N , ZA , ZX , INCX )
 COMPLEX * 16 ZA
 INTEGER INCX , N
 COMPLEX * 16 ZX ( * )
 INTEGER I , NINCX
 IF ( N .LE. 0 .OR. INCX .LE. 0 ) RETURN
 IF ( INCX .EQ. 1 ) THEN
 DO I = 1 , N
 ZX ( I ) = ZA * ZX ( I )
 END
 ELSE
 NINCX = N * INCX
 DO I = 1 , NINCX , INCX
 ZX ( I ) = ZA * ZX ( I )
 END
 END
 RETURN
 END
