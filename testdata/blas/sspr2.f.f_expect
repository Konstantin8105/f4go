 SUBROUTINE SSPR2 ( UPLO , N , ALPHA , X , INCX , Y , INCY , AP )
 REAL ALPHA
 INTEGER INCX , INCY , N
 CHARACTER UPLO
 REAL AP ( * ) , X ( * ) , Y ( * )
 REAL ZERO
 PARAMETER ( ZERO = 0.0E+0 )
 REAL TEMP1 , TEMP2
 INTEGER I , INFO , IX , IY , J , JX , JY , K , KK , KX , KY
 LOGICAL LSAME
 EXTERNAL LSAME
 EXTERNAL XERBLA
 INFO = 0
 IF ( .NOT. LSAME ( UPLO , 'U' ) .AND. .NOT. LSAME ( UPLO , 'L' ) ) THEN
 INFO = 1
 ELSE IF ( N .LT. 0 ) THEN
 INFO = 2
 ELSE IF ( INCX .EQ. 0 ) THEN
 INFO = 5
 ELSE IF ( INCY .EQ. 0 ) THEN
 INFO = 7
 END
 IF ( INFO .NE. 0 ) THEN
 CALL XERBLA ( 'SSPR2 ' , INFO )
 RETURN
 END
 IF ( ( N .EQ. 0 ) .OR. ( ALPHA .EQ. ZERO ) ) RETURN
 IF ( ( INCX .NE. 1 ) .OR. ( INCY .NE. 1 ) ) THEN
 IF ( INCX .GT. 0 ) THEN
 KX = 1
 ELSE
 KX = 1 - ( N - 1 ) * INCX
 END
 IF ( INCY .GT. 0 ) THEN
 KY = 1
 ELSE
 KY = 1 - ( N - 1 ) * INCY
 END
 JX = KX
 JY = KY
 END
 KK = 1
 IF ( LSAME ( UPLO , 'U' ) ) THEN
 IF ( ( INCX .EQ. 1 ) .AND. ( INCY .EQ. 1 ) ) THEN
 DO J = 1 , N
 IF ( ( X ( J ) .NE. ZERO ) .OR. ( Y ( J ) .NE. ZERO ) ) THEN
 TEMP1 = ALPHA * Y ( J )
 TEMP2 = ALPHA * X ( J )
 K = KK
 DO I = 1 , J
 AP ( K ) = AP ( K ) + X ( I ) * TEMP1 + Y ( I ) * TEMP2
 K = K + 1
 end

 END
 KK = KK + J
 end

 ELSE
 DO J = 1 , N
 IF ( ( X ( JX ) .NE. ZERO ) .OR. ( Y ( JY ) .NE. ZERO ) ) THEN
 TEMP1 = ALPHA * Y ( JY )
 TEMP2 = ALPHA * X ( JX )
 IX = KX
 IY = KY
 DO K = KK , KK + J - 1
 AP ( K ) = AP ( K ) + X ( IX ) * TEMP1 + Y ( IY ) * TEMP2
 IX = IX + INCX
 IY = IY + INCY
 end

 END
 JX = JX + INCX
 JY = JY + INCY
 KK = KK + J
 end

 END
 ELSE
 IF ( ( INCX .EQ. 1 ) .AND. ( INCY .EQ. 1 ) ) THEN
 DO J = 1 , N
 IF ( ( X ( J ) .NE. ZERO ) .OR. ( Y ( J ) .NE. ZERO ) ) THEN
 TEMP1 = ALPHA * Y ( J )
 TEMP2 = ALPHA * X ( J )
 K = KK
 DO I = J , N
 AP ( K ) = AP ( K ) + X ( I ) * TEMP1 + Y ( I ) * TEMP2
 K = K + 1
 end

 END
 KK = KK + N - J + 1
 end

 ELSE
 DO J = 1 , N
 IF ( ( X ( JX ) .NE. ZERO ) .OR. ( Y ( JY ) .NE. ZERO ) ) THEN
 TEMP1 = ALPHA * Y ( JY )
 TEMP2 = ALPHA * X ( JX )
 IX = JX
 IY = JY
 DO K = KK , KK + N - J
 AP ( K ) = AP ( K ) + X ( IX ) * TEMP1 + Y ( IY ) * TEMP2
 IX = IX + INCX
 IY = IY + INCY
 end

 END
 JX = JX + INCX
 JY = JY + INCY
 KK = KK + N - J + 1
 end

 END
 END
 RETURN
 END
