 SUBROUTINE ZTRMV ( UPLO , TRANS , DIAG , N , A , LDA , X , INCX )
 INTEGER INCX , LDA , N
 CHARACTER DIAG , TRANS , UPLO
 COMPLEX * 16 A ( LDA , * ) , X ( * )
 COMPLEX * 16 ZERO
 PARAMETER ( ZERO = ( 0.0D+0 , 0.0D+0 ) )
 COMPLEX * 16 TEMP
 INTEGER I , INFO , IX , J , JX , KX
 LOGICAL NOCONJ , NOUNIT
 LOGICAL LSAME
 EXTERNAL LSAME
 EXTERNAL XERBLA
 INTRINSIC DCONJG , MAX
 INFO = 0
 IF ( .NOT. LSAME ( UPLO , 'U' ) .AND. .NOT. LSAME ( UPLO , 'L' ) ) THEN
 INFO = 1
 ELSE IF ( .NOT. LSAME ( TRANS , 'N' ) .AND. .NOT. LSAME ( TRANS , 'T' ) .AND. .NOT. LSAME ( TRANS , 'C' ) ) THEN
 INFO = 2
 ELSE IF ( .NOT. LSAME ( DIAG , 'U' ) .AND. .NOT. LSAME ( DIAG , 'N' ) ) THEN
 INFO = 3
 ELSE IF ( N .LT. 0 ) THEN
 INFO = 4
 ELSE IF ( LDA .LT. MAX ( 1 , N ) ) THEN
 INFO = 6
 ELSE IF ( INCX .EQ. 0 ) THEN
 INFO = 8
 END
 IF ( INFO .NE. 0 ) THEN
 CALL XERBLA ( 'ZTRMV ' , INFO )
 RETURN
 END
 IF ( N .EQ. 0 ) RETURN
 NOCONJ = LSAME ( TRANS , 'T' )
 NOUNIT = LSAME ( DIAG , 'N' )
 IF ( INCX .LE. 0 ) THEN
 KX = 1 - ( N - 1 ) * INCX
 ELSE IF ( INCX .NE. 1 ) THEN
 KX = 1
 END
 IF ( LSAME ( TRANS , 'N' ) ) THEN
 IF ( LSAME ( UPLO , 'U' ) ) THEN
 IF ( INCX .EQ. 1 ) THEN
 DO J = 1 , N
 IF ( X ( J ) .NE. ZERO ) THEN
 TEMP = X ( J )
 DO I = 1 , J - 1
 X ( I ) = X ( I ) + TEMP * A ( I , J )
 end

 IF ( NOUNIT ) X ( J ) = X ( J ) * A ( J , J )
 END
 end

 ELSE
 JX = KX
 DO J = 1 , N
 IF ( X ( JX ) .NE. ZERO ) THEN
 TEMP = X ( JX )
 IX = KX
 DO I = 1 , J - 1
 X ( IX ) = X ( IX ) + TEMP * A ( I , J )
 IX = IX + INCX
 end

 IF ( NOUNIT ) X ( JX ) = X ( JX ) * A ( J , J )
 END
 JX = JX + INCX
 end

 END
 ELSE
 IF ( INCX .EQ. 1 ) THEN
 DO J = N , 1 , - 1
 IF ( X ( J ) .NE. ZERO ) THEN
 TEMP = X ( J )
 DO I = N , J + 1 , - 1
 X ( I ) = X ( I ) + TEMP * A ( I , J )
 end

 IF ( NOUNIT ) X ( J ) = X ( J ) * A ( J , J )
 END
 end

 ELSE
 KX = KX + ( N - 1 ) * INCX
 JX = KX
 DO J = N , 1 , - 1
 IF ( X ( JX ) .NE. ZERO ) THEN
 TEMP = X ( JX )
 IX = KX
 DO I = N , J + 1 , - 1
 X ( IX ) = X ( IX ) + TEMP * A ( I , J )
 IX = IX - INCX
 end

 IF ( NOUNIT ) X ( JX ) = X ( JX ) * A ( J , J )
 END
 JX = JX - INCX
 end

 END
 END
 ELSE
 IF ( LSAME ( UPLO , 'U' ) ) THEN
 IF ( INCX .EQ. 1 ) THEN
 DO J = N , 1 , - 1
 TEMP = X ( J )
 IF ( NOCONJ ) THEN
 IF ( NOUNIT ) TEMP = TEMP * A ( J , J )
 DO I = J - 1 , 1 , - 1
 TEMP = TEMP + A ( I , J ) * X ( I )
 end

 ELSE
 IF ( NOUNIT ) TEMP = TEMP * DCONJG ( A ( J , J ) )
 DO I = J - 1 , 1 , - 1
 TEMP = TEMP + DCONJG ( A ( I , J ) ) * X ( I )
 end

 END
 X ( J ) = TEMP
 end

 ELSE
 JX = KX + ( N - 1 ) * INCX
 DO J = N , 1 , - 1
 TEMP = X ( JX )
 IX = JX
 IF ( NOCONJ ) THEN
 IF ( NOUNIT ) TEMP = TEMP * A ( J , J )
 DO I = J - 1 , 1 , - 1
 IX = IX - INCX
 TEMP = TEMP + A ( I , J ) * X ( IX )
 end

 ELSE
 IF ( NOUNIT ) TEMP = TEMP * DCONJG ( A ( J , J ) )
 DO I = J - 1 , 1 , - 1
 IX = IX - INCX
 TEMP = TEMP + DCONJG ( A ( I , J ) ) * X ( IX )
 end

 END
 X ( JX ) = TEMP
 JX = JX - INCX
 end

 END
 ELSE
 IF ( INCX .EQ. 1 ) THEN
 DO J = 1 , N
 TEMP = X ( J )
 IF ( NOCONJ ) THEN
 IF ( NOUNIT ) TEMP = TEMP * A ( J , J )
 DO I = J + 1 , N
 TEMP = TEMP + A ( I , J ) * X ( I )
 end

 ELSE
 IF ( NOUNIT ) TEMP = TEMP * DCONJG ( A ( J , J ) )
 DO I = J + 1 , N
 TEMP = TEMP + DCONJG ( A ( I , J ) ) * X ( I )
 end

 END
 X ( J ) = TEMP
 end

 ELSE
 JX = KX
 DO J = 1 , N
 TEMP = X ( JX )
 IX = JX
 IF ( NOCONJ ) THEN
 IF ( NOUNIT ) TEMP = TEMP * A ( J , J )
 DO I = J + 1 , N
 IX = IX + INCX
 TEMP = TEMP + A ( I , J ) * X ( IX )
 end

 ELSE
 IF ( NOUNIT ) TEMP = TEMP * DCONJG ( A ( J , J ) )
 DO I = J + 1 , N
 IX = IX + INCX
 TEMP = TEMP + DCONJG ( A ( I , J ) ) * X ( IX )
 end

 END
 X ( JX ) = TEMP
 JX = JX + INCX
 end

 END
 END
 END
 RETURN
 END
