NEW_LINE            	

COMMENT             	*>
NEW_LINE            	

COMMENT             	*>      (DH11  DH12)    (1.D0  DH12)    (DH11  1.D0)    (1.D0  0.D0)
NEW_LINE            	

COMMENT             	*>    H=(          )    (          )    (          )    (          )
NEW_LINE            	

COMMENT             	*>      (DH21  DH22),   (DH21  1.D0),   (-1.D0 DH22),   (0.D0  1.D0).
NEW_LINE            	

COMMENT             	*>    SEE DROTMG FOR A DESCRIPTION OF DATA STORAGE IN DPARAM.
