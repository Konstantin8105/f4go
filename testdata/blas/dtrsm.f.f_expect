 SUBROUTINE DTRSM ( SIDE , UPLO , TRANSA , DIAG , M , N , ALPHA , A , LDA , B , LDB )
 DOUBLE PRECISION ALPHA
 INTEGER LDA , LDB , M , N
 CHARACTER DIAG , SIDE , TRANSA , UPLO
 DOUBLE PRECISION A ( LDA , * ) , B ( LDB , * )
 LOGICAL LSAME
 EXTERNAL LSAME
 EXTERNAL XERBLA
 INTRINSIC MAX
 DOUBLE PRECISION TEMP
 INTEGER I , INFO , J , K , NROWA
 LOGICAL LSIDE , NOUNIT , UPPER
 DOUBLE PRECISION ONE , ZERO


 ONE = 1.0D+0
 ZERO = 0.0D+0

 LSIDE = LSAME ( SIDE , 'L' )
 IF ( LSIDE ) THEN
 NROWA = M
 ELSE
 NROWA = N
 END
 NOUNIT = LSAME ( DIAG , 'N' )
 UPPER = LSAME ( UPLO , 'U' )
 INFO = 0
 IF ( ( .NOT. LSIDE ) .AND. ( .NOT. LSAME ( SIDE , 'R' ) ) ) THEN
 INFO = 1
 ELSE IF ( ( .NOT. UPPER ) .AND. ( .NOT. LSAME ( UPLO , 'L' ) ) ) THEN
 INFO = 2
 ELSE IF ( ( .NOT. LSAME ( TRANSA , 'N' ) ) .AND. ( .NOT. LSAME ( TRANSA , 'T' ) ) .AND. ( .NOT. LSAME ( TRANSA , 'C' ) ) ) THEN
 INFO = 3
 ELSE IF ( ( .NOT. LSAME ( DIAG , 'U' ) ) .AND. ( .NOT. LSAME ( DIAG , 'N' ) ) ) THEN
 INFO = 4
 ELSE IF ( M .LT. 0 ) THEN
 INFO = 5
 ELSE IF ( N .LT. 0 ) THEN
 INFO = 6
 ELSE IF ( LDA .LT. MAX ( 1 , NROWA ) ) THEN
 INFO = 9
 ELSE IF ( LDB .LT. MAX ( 1 , M ) ) THEN
 INFO = 11
 END
 IF ( INFO .NE. 0 ) THEN
 CALL XERBLA ( 'DTRSM ' , INFO )
 RETURN
 END
 IF ( M .EQ. 0 .OR. N .EQ. 0 ) RETURN
 IF ( ALPHA .EQ. ZERO ) THEN
 DO J = 1 , N
 DO I = 1 , M
 B ( I , J ) = ZERO
 end

 end

 RETURN
 END
 IF ( LSIDE ) THEN
 IF ( LSAME ( TRANSA , 'N' ) ) THEN
 IF ( UPPER ) THEN
 DO J = 1 , N
 IF ( ALPHA .NE. ONE ) THEN
 DO I = 1 , M
 B ( I , J ) = ALPHA * B ( I , J )
 end

 END
 DO K = M , 1 , - 1
 IF ( B ( K , J ) .NE. ZERO ) THEN
 IF ( NOUNIT ) B ( K , J ) = B ( K , J ) / A ( K , K )
 DO I = 1 , K - 1
 B ( I , J ) = B ( I , J ) - B ( K , J ) * A ( I , K )
 end

 END
 end

 end

 ELSE
 DO J = 1 , N
 IF ( ALPHA .NE. ONE ) THEN
 DO I = 1 , M
 B ( I , J ) = ALPHA * B ( I , J )
 end

 END
 DO K = 1 , M
 IF ( B ( K , J ) .NE. ZERO ) THEN
 IF ( NOUNIT ) B ( K , J ) = B ( K , J ) / A ( K , K )
 DO I = K + 1 , M
 B ( I , J ) = B ( I , J ) - B ( K , J ) * A ( I , K )
 end

 END
 end

 end

 END
 ELSE
 IF ( UPPER ) THEN
 DO J = 1 , N
 DO I = 1 , M
 TEMP = ALPHA * B ( I , J )
 DO K = 1 , I - 1
 TEMP = TEMP - A ( K , I ) * B ( K , J )
 end

 IF ( NOUNIT ) TEMP = TEMP / A ( I , I )
 B ( I , J ) = TEMP
 end

 end

 ELSE
 DO J = 1 , N
 DO I = M , 1 , - 1
 TEMP = ALPHA * B ( I , J )
 DO K = I + 1 , M
 TEMP = TEMP - A ( K , I ) * B ( K , J )
 end

 IF ( NOUNIT ) TEMP = TEMP / A ( I , I )
 B ( I , J ) = TEMP
 end

 end

 END
 END
 ELSE
 IF ( LSAME ( TRANSA , 'N' ) ) THEN
 IF ( UPPER ) THEN
 DO J = 1 , N
 IF ( ALPHA .NE. ONE ) THEN
 DO I = 1 , M
 B ( I , J ) = ALPHA * B ( I , J )
 end

 END
 DO K = 1 , J - 1
 IF ( A ( K , J ) .NE. ZERO ) THEN
 DO I = 1 , M
 B ( I , J ) = B ( I , J ) - A ( K , J ) * B ( I , K )
 end

 END
 end

 IF ( NOUNIT ) THEN
 TEMP = ONE / A ( J , J )
 DO I = 1 , M
 B ( I , J ) = TEMP * B ( I , J )
 end

 END
 end

 ELSE
 DO J = N , 1 , - 1
 IF ( ALPHA .NE. ONE ) THEN
 DO I = 1 , M
 B ( I , J ) = ALPHA * B ( I , J )
 end

 END
 DO K = J + 1 , N
 IF ( A ( K , J ) .NE. ZERO ) THEN
 DO I = 1 , M
 B ( I , J ) = B ( I , J ) - A ( K , J ) * B ( I , K )
 end

 END
 end

 IF ( NOUNIT ) THEN
 TEMP = ONE / A ( J , J )
 DO I = 1 , M
 B ( I , J ) = TEMP * B ( I , J )
 end

 END
 end

 END
 ELSE
 IF ( UPPER ) THEN
 DO K = N , 1 , - 1
 IF ( NOUNIT ) THEN
 TEMP = ONE / A ( K , K )
 DO I = 1 , M
 B ( I , K ) = TEMP * B ( I , K )
 end

 END
 DO J = 1 , K - 1
 IF ( A ( J , K ) .NE. ZERO ) THEN
 TEMP = A ( J , K )
 DO I = 1 , M
 B ( I , J ) = B ( I , J ) - TEMP * B ( I , K )
 end

 END
 end

 IF ( ALPHA .NE. ONE ) THEN
 DO I = 1 , M
 B ( I , K ) = ALPHA * B ( I , K )
 end

 END
 end

 ELSE
 DO K = 1 , N
 IF ( NOUNIT ) THEN
 TEMP = ONE / A ( K , K )
 DO I = 1 , M
 B ( I , K ) = TEMP * B ( I , K )
 end

 END
 DO J = K + 1 , N
 IF ( A ( J , K ) .NE. ZERO ) THEN
 TEMP = A ( J , K )
 DO I = 1 , M
 B ( I , J ) = B ( I , J ) - TEMP * B ( I , K )
 end

 END
 end

 IF ( ALPHA .NE. ONE ) THEN
 DO I = 1 , M
 B ( I , K ) = ALPHA * B ( I , K )
 end

 END
 end

 END
 END
 END
 RETURN
 END
