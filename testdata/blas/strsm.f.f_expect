 SUBROUTINE STRSM ( SIDE , UPLO , TRANSA , DIAG , M , N , ALPHA , A , LDA , B , LDB )
 REAL ALPHA
 INTEGER LDA , LDB , M , N
 CHARACTER DIAG , SIDE , TRANSA , UPLO
 REAL A ( LDA , * ) , B ( LDB , * )
 LOGICAL LSAME
 EXTERNAL LSAME
 EXTERNAL XERBLA
 INTRINSIC MAX
 REAL TEMP
 INTEGER I , INFO , J , K , NROWA
 LOGICAL LSIDE , NOUNIT , UPPER
 REAL ONE , ZERO
 PARAMETER ( ONE = 1.0E+0 , ZERO = 0.0E+0 )
 LSIDE = LSAME ( SIDE , 'L' )
 IF ( LSIDE ) THEN
 NROWA = M
 ELSE
 NROWA = N
 END
 NOUNIT = LSAME ( DIAG , 'N' )
 UPPER = LSAME ( UPLO , 'U' )
 INFO = 0
 IF ( ( .NOT. LSIDE ) .AND. ( .NOT. LSAME ( SIDE , 'R' ) ) ) THEN
 INFO = 1
 ELSE IF ( ( .NOT. UPPER ) .AND. ( .NOT. LSAME ( UPLO , 'L' ) ) ) THEN
 INFO = 2
 ELSE IF ( ( .NOT. LSAME ( TRANSA , 'N' ) ) .AND.
 + ( .NOT. LSAME ( TRANSA , 'T' ) ) .AND.
 + ( .NOT. LSAME ( TRANSA , 'C' ) ) ) THEN
 INFO = 3
 ELSE IF ( ( .NOT. LSAME ( DIAG , 'U' ) ) .AND. ( .NOT. LSAME ( DIAG , 'N' ) ) ) THEN
 INFO = 4
 ELSE IF ( M .LT. 0 ) THEN
 INFO = 5
 ELSE IF ( N .LT. 0 ) THEN
 INFO = 6
 ELSE IF ( LDA .LT. MAX ( 1 , NROWA ) ) THEN
 INFO = 9
 ELSE IF ( LDB .LT. MAX ( 1 , M ) ) THEN
 INFO = 11
 END
 IF ( INFO .NE. 0 ) THEN
 CALL XERBLA ( 'STRSM ' , INFO )
 RETURN
 END
 IF ( M .EQ. 0 .OR. N .EQ. 0 ) RETURN
 IF ( ALPHA .EQ. ZERO ) THEN
 DO 20 J = 1 , N
 DO 10 I = 1 , M
 B ( I , J ) = ZERO
 10 CONTINUE
 20 CONTINUE
 RETURN
 END
 IF ( LSIDE ) THEN
 IF ( LSAME ( TRANSA , 'N' ) ) THEN
 IF ( UPPER ) THEN
 DO 60 J = 1 , N
 IF ( ALPHA .NE. ONE ) THEN
 DO 30 I = 1 , M
 B ( I , J ) = ALPHA * B ( I , J )
 30 CONTINUE
 END
 DO 50 K = M , 1 , - 1
 IF ( B ( K , J ) .NE. ZERO ) THEN
 IF ( NOUNIT ) B ( K , J ) = B ( K , J ) / A ( K , K )
 DO 40 I = 1 , K - 1
 B ( I , J ) = B ( I , J ) - B ( K , J ) * A ( I , K )
 40 CONTINUE
 END
 50 CONTINUE
 60 CONTINUE
 ELSE
 DO 100 J = 1 , N
 IF ( ALPHA .NE. ONE ) THEN
 DO 70 I = 1 , M
 B ( I , J ) = ALPHA * B ( I , J )
 70 CONTINUE
 END
 DO 90 K = 1 , M
 IF ( B ( K , J ) .NE. ZERO ) THEN
 IF ( NOUNIT ) B ( K , J ) = B ( K , J ) / A ( K , K )
 DO 80 I = K + 1 , M
 B ( I , J ) = B ( I , J ) - B ( K , J ) * A ( I , K )
 80 CONTINUE
 END
 90 CONTINUE
 100 CONTINUE
 END
 ELSE
 IF ( UPPER ) THEN
 DO 130 J = 1 , N
 DO 120 I = 1 , M
 TEMP = ALPHA * B ( I , J )
 DO 110 K = 1 , I - 1
 TEMP = TEMP - A ( K , I ) * B ( K , J )
 110 CONTINUE
 IF ( NOUNIT ) TEMP = TEMP / A ( I , I )
 B ( I , J ) = TEMP
 120 CONTINUE
 130 CONTINUE
 ELSE
 DO 160 J = 1 , N
 DO 150 I = M , 1 , - 1
 TEMP = ALPHA * B ( I , J )
 DO 140 K = I + 1 , M
 TEMP = TEMP - A ( K , I ) * B ( K , J )
 140 CONTINUE
 IF ( NOUNIT ) TEMP = TEMP / A ( I , I )
 B ( I , J ) = TEMP
 150 CONTINUE
 160 CONTINUE
 END
 END
 ELSE
 IF ( LSAME ( TRANSA , 'N' ) ) THEN
 IF ( UPPER ) THEN
 DO 210 J = 1 , N
 IF ( ALPHA .NE. ONE ) THEN
 DO 170 I = 1 , M
 B ( I , J ) = ALPHA * B ( I , J )
 170 CONTINUE
 END
 DO 190 K = 1 , J - 1
 IF ( A ( K , J ) .NE. ZERO ) THEN
 DO 180 I = 1 , M
 B ( I , J ) = B ( I , J ) - A ( K , J ) * B ( I , K )
 180 CONTINUE
 END
 190 CONTINUE
 IF ( NOUNIT ) THEN
 TEMP = ONE / A ( J , J )
 DO 200 I = 1 , M
 B ( I , J ) = TEMP * B ( I , J )
 200 CONTINUE
 END
 210 CONTINUE
 ELSE
 DO 260 J = N , 1 , - 1
 IF ( ALPHA .NE. ONE ) THEN
 DO 220 I = 1 , M
 B ( I , J ) = ALPHA * B ( I , J )
 220 CONTINUE
 END
 DO 240 K = J + 1 , N
 IF ( A ( K , J ) .NE. ZERO ) THEN
 DO 230 I = 1 , M
 B ( I , J ) = B ( I , J ) - A ( K , J ) * B ( I , K )
 230 CONTINUE
 END
 240 CONTINUE
 IF ( NOUNIT ) THEN
 TEMP = ONE / A ( J , J )
 DO 250 I = 1 , M
 B ( I , J ) = TEMP * B ( I , J )
 250 CONTINUE
 END
 260 CONTINUE
 END
 ELSE
 IF ( UPPER ) THEN
 DO 310 K = N , 1 , - 1
 IF ( NOUNIT ) THEN
 TEMP = ONE / A ( K , K )
 DO 270 I = 1 , M
 B ( I , K ) = TEMP * B ( I , K )
 270 CONTINUE
 END
 DO 290 J = 1 , K - 1
 IF ( A ( J , K ) .NE. ZERO ) THEN
 TEMP = A ( J , K )
 DO 280 I = 1 , M
 B ( I , J ) = B ( I , J ) - TEMP * B ( I , K )
 280 CONTINUE
 END
 290 CONTINUE
 IF ( ALPHA .NE. ONE ) THEN
 DO 300 I = 1 , M
 B ( I , K ) = ALPHA * B ( I , K )
 300 CONTINUE
 END
 310 CONTINUE
 ELSE
 DO 360 K = 1 , N
 IF ( NOUNIT ) THEN
 TEMP = ONE / A ( K , K )
 DO 320 I = 1 , M
 B ( I , K ) = TEMP * B ( I , K )
 320 CONTINUE
 END
 DO 340 J = K + 1 , N
 IF ( A ( J , K ) .NE. ZERO ) THEN
 TEMP = A ( J , K )
 DO 330 I = 1 , M
 B ( I , J ) = B ( I , J ) - TEMP * B ( I , K )
 330 CONTINUE
 END
 340 CONTINUE
 IF ( ALPHA .NE. ONE ) THEN
 DO 350 I = 1 , M
 B ( I , K ) = ALPHA * B ( I , K )
 350 CONTINUE
 END
 360 CONTINUE
 END
 END
 END
 RETURN
 END
